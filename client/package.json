  "homepage": "https://www.bulmag.bg",
  "scripts": {
    "start": "SET PORT=34881 && node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/bulmag-design-system-react.scss src/assets/css/bulmag-design-system-react.css",
    "minify-sass": "node-sass src/assets/scss/bulmag-design-system-react.scss src/assets/css/bulmag-design-system-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/bulmag-design-system-react.scss src/assets/css/bulmag-design-system-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/core": "7.4.3",
    "@grapecity/spread-sheets-react": "^13.0.3",
    "@svgr/webpack": "4.1.0",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "axios": "^0.19.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
